{
  "name": "next-trpc-drgaph",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p database:dev dev2",
    "dev2": "sleep 20 && run-p -c graphql:dev types:dev web:dev",
    "build": "run-s graphql:build web:build",
    "start": "run-p database:dev web:start",
    "lint": "next lint",
    "database:dev": "docker-compose up",
    "database:start": "docker-compose up -d",
    "types:check": "tsc --noEmit",
    "types:dev": "tsc --noEmit -w",
    "web:dev": "next dev",
    "web:build": "next build",
    "web:start": "next start",
    "graphql:build": "graphql-codegen -r ./env.js",
    "graphql:dev": "nodemon -e graphql --exec yarn graphql:build",
    "schema:push": "node ./schema.js push http://localhost:8080 ./schema.graphql",
    "schema:validate": "node ./schema.js validate http://localhost:8080 ./schema.graphql",
    "test": "vitest"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@bluesky-digital-labs/next-firebase-auth": "^0.8.0",
    "@fontsource/oswald": "^4.5.9",
    "@fontsource/raleway": "^4.5.9",
    "@react-icons/all-files": "^4.1.0",
    "@sentry/nextjs": "^7.7.0",
    "@trpc/client": "^10.0.0-alpha.38",
    "@trpc/next": "^10.0.0-alpha.38",
    "@trpc/react": "^10.0.0-alpha.38",
    "@trpc/server": "^10.0.0-alpha.38",
    "axios": "^0.27.2",
    "cookie": "^0.5.0",
    "firebase-admin": "^11.0.0",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "next": "12.2.2",
    "next-plausible": "^3.2.0",
    "next-seo": "^5.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.33.1",
    "react-is": "^18.2.0",
    "react-query": "^3.39.2",
    "sharp": "^0.30.7",
    "styled-components": "^5.3.5",
    "zod": "^3.17.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.8.1",
    "@graphql-codegen/near-operation-file-preset": "^2.3.1",
    "@graphql-codegen/typescript": "^2.7.1",
    "@graphql-codegen/typescript-generic-sdk": "^2.4.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@next/env": "^13.0.7",
    "@types/cookie": "^0.5.1",
    "@types/node": "18.0.6",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.25",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "typescript": "4.7.4",
    "vitest": "^0.19.1"
  }
}
